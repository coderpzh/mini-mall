/* pages/detail/childCnps/w-navBar/w-navBar.wxss */
.navBar{
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  height: 80rpx;
  border-bottom: 8rpx solid #eee;
}

.nav-bar{
  display: flex;
  width: 80%;
  margin: 0 auto;
}

.nav-item{
  flex: 1;
  text-align: center;
  padding: 20rpx 0;
}

.active{
  color: hotpink;
}